{
    "name": "ekup/yii2-installer",
    "type": "library",
    "description": "Yii2 installer-configurator",
    "keywords": [
        "ekup",
        "yii2-installer"
    ],
    "homepage": "https://github.com/ekup/yii2-installer",
    "license": "MIT",
    "authors": [
        {
            "name": "EKup",
            "email": "ekup73@gmail.com",
            "homepage": "https://github.com/ekup/",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "ekup\\yii2\\installer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ekup\\yii2\\installer\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.0-dev"
        }
    }
}
